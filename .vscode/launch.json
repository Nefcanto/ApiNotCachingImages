{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "debug",
            "program": "/Sceo/SiteApi/bin/Api.dll",
            "args": [],
            "cwd": "/Sceo/SiteApi",
            "console": "internalConsole",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "${env:Host}."
            },
            "logging": {
                "moduleLoad": false
            },
            "stopAtEntry": false
        }
    ]
}
